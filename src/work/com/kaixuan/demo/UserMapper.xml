<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="work.com.kaixuan.demo.UserMapper">
    <select id="selectUserById" resultType="work.com.kaixuan.demo.UserModel">
         select name,age,job from shop_test_hello
    </select>

    <insert id="insertUser" parameterType="java.util.Map">
           insert into  shop_test_hello(name,age,job) values (  #{name},#{age},#{job} )
    </insert>

</mapper>
